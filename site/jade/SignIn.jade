table#existingUser
	tbody
		tr
			td
			td
				|Sign In
		tr
			td
				|Usename
			td
				input#UserName(type="text")
				ul(data-bind="foreach: SignInUserNameError")
					li(data-bind="text: $data" style="color : red;")
		tr
			td
				|Password
			td
				input#Password(type="password")
				ul(data-bind="foreach: SignInPassError")
					li(data-bind="text: $data" style="color : red;")
		tr
			td
			td
				#SignIn.btn.btn-default(data-bind="click: SignIn")
					|Submit
				#Join.btn.btn-default(data-bind="click: ShowJoin")
					|Join